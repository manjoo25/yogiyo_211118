<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yogiyo.menu.dao.MenuDAO">
	<select id="selectMenuList" resultType="com.yogiyo.menu.model.Menu">
		SELECT
			`id`
			,`storeId`
			,`name`
			,`introduce`
			,`imagePath`
			,`price`
			,`createdAt`
			,`updatedAt`
		FROM
			`menu`
	</select>
</mapper>