<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yogiyo.store.dao.StoreDAO">
	<select id="selectStoreList" resultType="com.yogiyo.store.model.Store">
		SELECT
			`id`
			,`name`
			,`reviewPoint`
			,`address`
			,`introduce`
			,`category`
			,`imagePath`
			,`createdAt`
			,`updatedAt`
		FROM
			`store`
	</select>
</mapper>